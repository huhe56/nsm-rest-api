<?xml version="1.0" encoding="UTF-8"?>
<serviceOffering xmlns="http://www.cisco.com/NetworkServicesManager/1.1"
	xmlns:ns2="http://www.w3.org/2005/Atom">
	<uid>${uid}</uid>
	<name>${name}</name>
	<description>${description}</description>
	<parameters>
		<parameter>
			<name>global.pool</name>
			<type>subnets</type>
			<subnets>
				<subnet>
					<ipv4>${global.pool.subnet.1.ipv4}</ipv4>
					<mask>${global.pool.subnet.1.mask}</mask>
				</subnet>
			</subnets>
		</parameter>
	</parameters>
</serviceOffering>