<?xml version="1.0" encoding="UTF-8"?>
<ipReservation xmlns="http://www.cisco.com/NetworkServicesManager/1.1"
	xmlns:ns2="http://www.w3.org/2005/Atom">
	<uid>${uid}</uid>
	<name>${name}</name>
	<description>${description}</description>
	<owner>${owner}</owner>
	<ipv4AddressRanges>
		<ipv4AddressRange>
			<ipv4Start>${ipv4Start}</ipv4Start>
			<ipv4End>${ipv4End}</ipv4End>
		</ipv4AddressRange>
	</ipv4AddressRanges>
</ipReservation>