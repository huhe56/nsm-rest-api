<?xml version="1.0" encoding="UTF-8"?>
<serviceOffering xmlns="http://www.cisco.com/NetworkServicesManager/1.1"
	xmlns:ns2="http://www.w3.org/2005/Atom">
	<uid>${uid}</uid>
	<name>${name}</name>
	<description>${description}</description>
	<parameters>
		<parameter>
			<name>external.networks</name>
			<type>uids</type>
			<uids>
				<uid>${external.networks.1.uid}</uid>
			</uids>
		</parameter>
		<parameter>
			<name>assigned.zone</name>
			<type>uid</type>
			<uid>${assigned.zone.uid}</uid>
		</parameter>
		<parameter>
			<name>mpls.export.routes</name>
			<type>routeTargets</type>
			<routeTargets>
				<routeTarget>
					<ipv4>${mpls.export.routes.routeTarget.1.ipv4}</ipv4>
					<ASNumber>${mpls.export.routes.routeTarget.1.ASNumber}</ASNumber>
				</routeTarget>
			</routeTargets>
		</parameter>
		<parameter>
			<name>mpls.import.routes</name>
			<type>routeTargets</type>
			<routeTargets>
				<routeTarget>
					<ipv4>${mpls.import.routes.routeTarget.1.ipv4}</ipv4>
					<ASNumber>${mpls.import.routes.routeTarget.1.ASNumber}</ASNumber>
				</routeTarget>
			</routeTargets>
		</parameter>
	</parameters>
</serviceOffering>