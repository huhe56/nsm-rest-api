<?xml version="1.0" encoding="UTF-8"?>
<serviceOffering xmlns="http://www.cisco.com/NetworkServicesManager/1.1"
	xmlns:ns2="http://www.w3.org/2005/Atom">
	<uid>${uid}</uid>
	<name>${name}</name>
	<description>${description}</description>
	<parameters>
		<parameter>
			<name>lb.traffic.src</name>
			<type>trafficFilters</type>
			<trafficFilters>
				<trafficFilter>
					<uid>${traffic.src.filter.1.uid}</uid>
				</trafficFilter>
			</trafficFilters>
		</parameter>
		<parameter>
			<name>real.servers</name>
			<trafficFilters>
				<trafficFilter>
					<uid>${real.servers.trafficFilter.1.uid}</uid>
					<ipv4AddressRanges>
						<ipv4AddressRange>
							<ipv4Start>${real.servers.1.ipv4.start}</ipv4Start>
							<ipv4End>${real.servers.1.ipv4.end}</ipv4End>
						</ipv4AddressRange>
						<ipv4AddressRange>
							<ipv4Start>${real.servers.2.ipv4.start}</ipv4Start>
							<ipv4End>${real.servers.2.ipv4.end}</ipv4End>
						</ipv4AddressRange>
						<ipv4AddressRange>
							<ipv4Start>${real.servers.3.ipv4.start}</ipv4Start>
							<ipv4End>${real.servers.3.ipv4.end}</ipv4End>
						</ipv4AddressRange>
					</ipv4AddressRanges>
				</trafficFilter>
			</trafficFilters>
		</parameter>
		<parameter>
			<name>lb.service.ports</name>
			<type>protocolServices</type>
			<protocolServices>
				<protocolService>
					<protocol>${lb.service.1.protocol}</protocol>
					<portRange>
						<start>${lb.service.1.port.start}</start>
					</portRange>
				</protocolService>
				<protocolService>
					<protocol>${lb.service.2.protocol}</protocol>
					<portRange>
						<start>${lb.service.2.port.start}</start>
					</portRange>
				</protocolService>
			</protocolServices>
		</parameter>
		<parameter>
			<name>vip.address</name>
			<trafficFilter>
				<uid>${vip.address.trafficFilter.uid}</uid>
				<ipv4AddressRanges>
					<ipv4AddressRange>
						<ipv4Start>${vip.address.1.ipv4.start}</ipv4Start>
						<ipv4End>${vip.address.1.ipv4.end}</ipv4End>
					</ipv4AddressRange>
				</ipv4AddressRanges>
			</trafficFilter>
		</parameter>
		<parameter>
			<name>lb.algorithm</name>
			<string>${lb.algorithm}</string>
		</parameter>
		<parameter>
			<name>serverfarm.probe</name>
			<strings>
				<string>${serverfarm.probe.1}</string>
				<string>${serverfarm.probe.2}</string>
			</strings>
		</parameter>
	</parameters>
</serviceOffering>